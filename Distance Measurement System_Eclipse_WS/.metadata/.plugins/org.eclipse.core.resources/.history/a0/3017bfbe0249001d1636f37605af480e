#include "lcd.h"
#include"avr/io.h"
#include"ultrasonic.h"
int main() {
	uint8 dist;
	SREG |= (1 << 7);
	Ultrasonic_init();
	LCD_init();
	LCD_displayString("Distance= ");
	while (1) {
		dist=Ultrasonic_readDistance();
		if (dist >= 100) {

		} else {
			// some code
			LCD_displayCharacter(' ');
		}
	}
}
