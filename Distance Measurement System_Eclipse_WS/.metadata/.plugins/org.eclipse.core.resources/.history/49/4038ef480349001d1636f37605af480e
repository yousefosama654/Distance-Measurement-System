#include "lcd.h"
#include"avr/io.h"
#include"ultrasonic.h"
int main() {
	uint8 dist=0;
	SREG |= (1 << 7);
	Ultrasonic_init();
	LCD_init();
	LCD_displayString("Distance= ");
	while (1) {
//		dist = Ultrasonic_readDistance();
		if (dist < 100) {
			LCD_moveCursor(0, 9);
			LCD_intgerToString(101);
			LCD_displayString("Cm");
		} else {
			// some code
			LCD_moveCursor(0, 9);
			LCD_intgerToString(10);
			LCD_displayCharacter(' ');
			LCD_displayString("Cm");
		}
	}
}
